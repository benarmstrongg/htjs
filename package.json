{
    "name": "@barndev/htjs",
    "version": "1.0.1",
    "description": "Lightweight, no build, plain JS alternative to JSX",
    "license": "MIT",
    "keywords": [
        "htjs",
        "jsx",
        "preact"
    ],
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": "./dist/index.js",
        "./elems": "./dist/elems.js"
    },
    "author": "ben armstrong",
    "repository": {
        "type": "git",
        "url": "https://github.com/benarmstrongg/htjs.git"
    },
    "files": [
        "./dist/**",
        "./elems*"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "dev": "nodemon -w src -e .ts -x 'pnpm build && serve --cors dist'",
        "build": "tsup",
        "test": "jest",
        "lint": "tsc",
        "changeset": "changeset",
        "prepublish": "pnpm run build && cp ./dist/elems* ./",
        "publish": "pnpm run prepublish && changeset publish"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.1",
        "@jest/globals": "^29.7.0",
        "@types/node": "^20.11.19",
        "jest": "^29.7.0",
        "nodemon": "^3.1.0",
        "serve": "^14.2.1",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "tsup": "^8.0.2",
        "typescript": "^5.3.3"
    }
}
