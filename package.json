{
    "name": "@barndev/htjs",
    "version": "0.0.5",
    "description": "Lightweight, pure-JS alternative to JSX",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": "./dist/index.js",
        "./elems": "./dist/elems.js"
    },
    "author": "ben armstrong",
    "repository": {
        "type": "git",
        "url": "https://github.com/benarmstrongg/htjs.git"
    },
    "license": "MIT",
    "keywords": [
        "htjs",
        "jsx",
        "preact"
    ],
    "files": [
        "dist/**"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsup",
        "lint": "tsc",
        "changeset": "changeset",
        "publish": "pnpm run build && changeset publish"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.1",
        "@types/node": "^20.11.19",
        "ts-node": "^10.9.2",
        "tsup": "^8.0.2",
        "typescript": "^5.3.3"
    }
}
