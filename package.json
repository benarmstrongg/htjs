{
    "name": "@barndev/htjs",
    "version": "1.0.3",
    "description": "Lightweight, no build, plain JS alternative to JSX",
    "license": "MIT",
    "keywords": [
        "htjs",
        "jsx",
        "preact"
    ],
    "main": "index.js",
    "module": "index.mjs",
    "types": "index.d.ts",
    "exports": {
        ".": "./index.js",
        "./elems": "./elems.js"
    },
    "author": "ben armstrong",
    "repository": {
        "type": "git",
        "url": "https://github.com/benarmstrongg/htjs.git"
    },
    "files": [
        "./index*",
        "./elems*"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "dev": "nodemon -w src -e .ts -x 'pnpm build && serve --cors dist'",
        "build": "tsup",
        "test": "jest",
        "lint": "tsc",
        "changeset": "changeset",
        "prepublish": "pnpm run build && cp ./dist/* ./",
        "publish": "pnpm run prepublish && changeset publish"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.1",
        "@jest/globals": "^29.7.0",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.19",
        "@types/react": "^18.2.61",
        "jest": "^29.7.0",
        "nodemon": "^3.1.0",
        "preact": "^10.19.6",
        "react": "^18.2.0",
        "serve": "^14.2.1",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "tsup": "^8.0.2",
        "typescript": "^5.3.3"
    }
}
